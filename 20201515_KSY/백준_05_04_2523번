//실습1_별 찍기 - 13
#include <iostream>
using namespace std;

int main(void) {
	cin.tie(NULL);
	ios::sync_with_stdio(false);

	int n; 
	cin >> n; 
	for (int i = 0; i < n; i++) 
	{ 
		for (int j = 0; j <= i; j++) 
		{ 
			cout << "*"; 
		} 
		cout << '\n'; 
	} 
	for (int i = n - 1; i > 0; i--) 
	{ 
		for (int j = 0; j < i; j++) 
		{ 
			cout << "*"; 
		} 
		cout << '\n';
	}

	
	return 0;
}
