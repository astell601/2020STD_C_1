//실습1_세 수
#include <iostream>
using namespace std;

int main(void) {
	cin.tie(NULL);
	ios::sync_with_stdio(false);

    int a, b, c, mid;
    cin >> a >> b >> c;

    if (a > b) {
        if (c > a)
        {
            cout << a;
            return 0;
        }
        else if (c > b)
        {
            cout << c;
            return 0;
        }
        else
        {
            cout << b;
            return 0;
        }
        if (b > c)
        {
            cout << b;
            return 0;
        }
        else if (a > c)
        {
            cout << c;
            return 0;
        }
        else {
            cout << a;
            return 0;
        }
    }
    else {
        if (c > b)
        {
            cout << b;
            return 0;
        }
        else if (c > a)
        {
            cout << c;
            return 0;
        }
        else {
            cout << a;
            return 0;
        }
        if (a > c)
        {
            cout << a;
            return 0;
        }
        else if (b > c)
        {
            cout << c;
            return 0;
        }
        else {
            cout << b;
            return 0;
        }
    }
	
	return 0;
}
